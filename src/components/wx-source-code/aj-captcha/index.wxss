/*postcss-pxtransform disable*/
.aj-captcha-mask{
    position: fixed;
    z-index: 1001;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,.7);
    transition: all .5s;
}
.fixed .aj-captcha-mask{
  display: none;
}
.aj-captcha{
  width: 660rpx;
  position: fixed;
  z-index: 1002;
  box-sizing: border-box;
  border-radius: 20rpx;
  border: 1px solid #e4e7eb;
  background-color: #fff;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.fixed .aj-captcha{
  position: relative;
  border: none;
  left: 0;
  top: 0;
  background: none;
  box-shadow: none;
  transform: none;
}
.verifybox-top{
  padding: 40rpx 30rpx 30rpx;
  text-align: left;
  font-size: 34rpx;
  color: #333;
  box-sizing: border-box;
  font-weight: 500;
}
.fixed .verifybox-top{
  display: none;
}
.verifybox-close {
  position: absolute;
  top: 16rpx;
  right: 18rpx;
  padding: 16rpx;
  height: 60rpx;
  width: 60rpx;
  text-align: center;
  background-image: url("data:image/png;base64,");
  background-size : cover;
}

.verifybox-bottom{
  box-sizing: border-box;
  padding-bottom: 30rpx;
}
.fixed .verifybox-bottom{
  padding: 0;
}
.verify-img-panel {
  margin: 0 auto;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  position: relative;
}
.click-word-point{
  position:absolute;
  background-color: #1abd6c;
  color: #fff;
  z-index: 4;
  width: 20px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  border-radius: 50%;
}
.verify-bar-area {
  position: relative;
  text-align: center;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  -webkit-border-radius: 40rpx;
  background: #E8E8E8;
  border-radius: 40rpx;
  margin: 0 auto;
}
.suc-area.verify-bar-area {
  color: #4cae4c;
  border-color: #5cb85c;
}
.err-area.verify-bar-area {
  color: #d9534f;
  border-color: #d9534f;
}
.verify-msg {
  z-index: 3;
}
.verify-left-bar {
  position: absolute;
  top: -10px;
  left: -1px;
  background: #f0fff0;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  border-radius: 2px;
  -webkit-border-radius: 2px;
}
.verify-move-block {
  position: absolute;
  top: 0;
  left: 0;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  width: 130rpx;
  height: 70rpx;
  background-image: url('data:image/png;base64,');
  background-size: cover;
}
.status-1.verify-left-bar{
  border-color: rgb(255, 255, 255);
  width: 0px!important;
  transition: width .3s ease;
}
.status-2.verify-left-bar{
  border-color: rgb(51, 122, 183);
}
.status-3.verify-left-bar{
  border-color: rgb(217, 83, 79);
}
.status-4.verify-left-bar{
  border-color: rgb(92, 184, 92);
}
.verify-sub-block {
  position: absolute;
  text-align: center;
  z-index: 3;
}
.refreshWrap {
  position: relative;
  margin: 20px auto 0;
  padding-bottom: 30px;
  overflow: hidden;
}

.verify-tips {
  position: absolute;
  left: 0;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  bottom: -50px;
  transition: bottom .4s linear;
  padding-left: 50rpx;
  background-size: 36rpx 36rpx;
  background-position: left center;
  background-repeat: no-repeat;
}
.err-bg {
  bottom: 0;
  color: #FF4D4D;
  background-image: url('data:image/png;base64,');
}
.suc-bg {
  bottom: 0;
  color: #52C41A;
  background-image: url('data:image/png;base64,');
}

.verify-refresh {
  position: absolute;
  top: 10rpx;
  right: 0;
  z-index: 3;
  width: 36rpx;
  height: 36rpx;
  padding: 10rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center center;
}


/* right-black.png 黑色 */
.status-1 .verify-move-block{
  transform: translateX(0px)!important;
  transition: transform .3s ease;
}
